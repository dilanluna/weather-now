{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@screens/*": ["./src/screens/*"],
      "@constants": ["./src/constants"],
      "@contexts/*": ["./src/contexts/*"],
      "@features/*": ["./src/features/*"],
      "@components/*": ["./src/components/*"]
